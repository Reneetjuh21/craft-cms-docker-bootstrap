{
  "require": {
    "craftcms/cms": "^4.3.5",
    "craftcms/redactor": "3.0.3",
    "ether/seo": "v4.0.3",
    "huttchdev/craft-webpme": "1.0.0",
    "nystudio107/craft-twigpack": "4.0.0-beta.4",
    "presseddigital/linkit": "4.0.4.1",
    "solspace/craft-express-forms": "2.0.1",
    "verbb/image-resizer": "3.0.4",
    "verbb/super-table": "3.0.7",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^1.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
